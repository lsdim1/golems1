// Program that calculates the area of a rectangle

const length = parseFloat(prompt("Enter the length of the rectangle: "));
const width = parseFloat(prompt("Enter the width of the rectangle: "));

const area = length * width;

console.log(`The area of the rectangle is ${area}`);



// This program calculates the area of a rectangle

const length = parseFloat(prompt("Enter the length of the rectangle: ")); // Get input from user and convert it to a float
const width = parseFloat(prompt("Enter the width of the rectangle: ")); // Get input from user and convert it to a float

const area = length * width; // Calculate the area of the rectangle

console.log(`The area of the rectangle is ${area}`); // Print the result to the console
